<form action="/quizes">
    <label>Buscar preguntas</label>
    <input type="text" name="search" value=""/>
    <input type="submit" value="buscar"/>
</form>

<table>
    <% if (quizes.length === 0) { %>
        <tr>
            <td>No hay preguntas.</td>
        </tr>
    <% } %>

    <% var i; for(i=0; i < quizes.length; i++) { %>
    <tr>
        <td><a href="/quizes/<%= quizes[i].id %>">
                <%= quizes[i].pregunta %>
            </a>
        </td>
    </tr>
    <% } %>
</table>